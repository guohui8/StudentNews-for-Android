<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="LiteListView">
        <attr name="Text" format="reference|string"></attr>
        <attr name="Oriental">
            <enum name="Horizontal" value="1"></enum>
            <enum name="Vertical" value="0"></enum>
        </attr>
        <attr name="alwaysShowDivider" format="boolean" />
    </declare-styleable>
    <declare-styleable name="ZYBasicItem">
        <attr name="title" format="string" />
        <attr name="subTitle" format="string" />
        <attr name="input" format="string" />
        <attr name="input_hint" format="string" />
        <attr name="input_type" format="integer">
            <flag name="text" value="0x00000001" />
            <flag name="password" value="0x00000080" />
            <flag name="number" value="0x00000002" />
            <flag name="phone" value="0x00000003" />
            <flag name="email" value="0x00000020" />
        </attr>
        <attr name="input_maxLength" format="integer" />
        <attr name="count" format="string" />
        <attr name="checked" format="integer">
            <enum name="check" value="1" />
            <enum name="uncheck" value="2" />
        </attr>
        <attr name="clickables" format="boolean" />
        <attr name="title_textType" format="integer">
            <flag name="small" value="0x00000001" />
            <flag name="yellow_color" value="0x00000002" />
            <flag name="gray_color" value="0x00000004" />
            <flag name="black_color" value="0x00000008" />
            <flag name="bold" value="0x00000010" />
        </attr>
        <attr name="subTitle_textType" format="integer">
            <flag name="small" value="0x00000001" />
            <flag name="yellow_color" value="0x00000002" />
            <flag name="gray_color" value="0x00000004" />
            <flag name="black_color" value="0x00000008" />
            <flag name="bold" value="0x00000010" />
        </attr>
        <attr name="count_textType" format="integer">
            <flag name="small" value="0x00000001" />
            <flag name="yellow_color" value="0x00000002" />
            <flag name="gray_color" value="0x00000004" />
            <flag name="black_color" value="0x00000008" />
            <flag name="bold" value="0x00000010" />
        </attr>
        <attr name="input_textType" format="integer">
            <flag name="small" value="0x00000001" />
            <flag name="yellow_color" value="0x00000002" />
            <flag name="gray_color" value="0x00000004" />
            <flag name="black_color" value="0x00000008" />
            <flag name="bold" value="0x00000010" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RankTextView">
        <attr name="text1" format="string" />
        <attr name="text2" format="string" />
        <attr name="textSize1" format="dimension" />
        <attr name="textSize2" format="dimension" />
        <attr name="rectH" format="dimension" />
    </declare-styleable>
    <declare-styleable name="SnsSettingItemView">
        <attr name="line_1" format="reference|string" />
        <attr name="line_2" format="reference|string" />
        <attr name="line_mode">
            <enum name="single_line" value="1" />
            <enum name="double_line" value="2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="UserScoreBar">
        <attr name="barColor1" format="color" />
        <attr name="barColor2" format="color" />
    </declare-styleable>
    <declare-styleable name="NetworkImageView">
        <attr name="filterBitmap" format="boolean" />
        <attr name="requireBeforeAttach" format="boolean" />
        <attr name="placeholderEmpty" format="reference" />
        <attr name="placeholderLoading" format="reference" />
        <attr name="placeholderError" format="reference" />
    </declare-styleable>
    <declare-styleable name="ViewfinderView"></declare-styleable>

</resources>